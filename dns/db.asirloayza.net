; zone file asirloayza.net
$TTL    604800
@          IN   SOA    dns.asirloayza.net. root.asirloayza.net. (
                 1      ; serial number
            604800      ; refresh
            86400       ; retry
            2419200     ; expire
            604800 )    ; negative cache TTL
; Servidores DNS del dominio
@              IN    NS    dns.asirloayza.net.
dns            IN    A     192.168.8.2

; Hosts
dhcp       IN    A    192.168.8.3
apache     IN    A    192.168.8.4
mysql      IN    A    192.168.8.5
@          IN    A    192.168.8.4